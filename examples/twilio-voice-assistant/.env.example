# Twilio Voice Assistant Configuration
# Copy this file to .env and fill in your values

# Server Configuration
PORT=8080

# Twilio Stream URL (your public WebSocket endpoint)
# Example: wss://your-domain.ngrok.io/media
TWILIO_STREAM_URL=wss://your-domain.com/media

# OpenAI Configuration
OPENAI_API_KEY=sk-your-openai-api-key

# ElevenLabs Configuration
ELEVENLABS_API_KEY=your-elevenlabs-api-key
# Voice ID (default: Rachel - 21m00Tcm4TlvDq8ikWAM)
# Find more voices at https://api.elevenlabs.io/v1/voices
ELEVENLABS_VOICE_ID=21m00Tcm4TlvDq8ikWAM

# VAD Configuration
VAD_ENABLED=true
VAD_MODEL_PATH=models/silero_vad.onnx

# Assistant System Prompt (optional)
# SYSTEM_PROMPT=You are a helpful AI phone assistant...
