# ONNX Runtime environment for VAD support
# This file is automatically loaded by direnv when entering this directory

if command -v brew &> /dev/null && brew --prefix onnxruntime &> /dev/null; then
    ORT_PREFIX="$(brew --prefix onnxruntime)"
    export CGO_CFLAGS="-I$ORT_PREFIX/include/onnxruntime"
    export CGO_LDFLAGS="-L$ORT_PREFIX/lib -lonnxruntime"
    # echo "ONNX Runtime environment configured: $ORT_PREFIX"
fi
